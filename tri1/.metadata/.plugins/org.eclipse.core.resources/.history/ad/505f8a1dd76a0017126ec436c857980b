package com.triffil2;

public class C {

	public static void main(String[] args) {
		
		String s1="th2is2";
		String s2="i2s";
		String s3="m2y";
		String s4="num3ber2";
		System.out.println(s1+" "+s2+" "+s3+" "+s4);
		
		int sum=0;
		String s5;
		char c;
        for(int i = 0; i < s1.length() ; i++){
        	if(Character.isLetter(i)){
        		c=s1.charAt(i);
        		System.out.println(c);
        	}
        }
	}

}
