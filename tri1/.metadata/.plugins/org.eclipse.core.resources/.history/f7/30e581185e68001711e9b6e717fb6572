package com.triffil;

import java.util.Scanner;

public class Matrix
{
public static void main(String args[]){
	int[][] a=new int[2][2];
	int[][] b=new int[2][2];
	 Scanner s=new Scanner(System.in);  
	 
	
	for(int i=0;i<3;i++){  
		 for(int j=0;j<3;j++){  
		   System.out.print(a[i][j]+" "+b[i][j]);  
		 }  
		 System.out.println();  
		}  
	


}
}