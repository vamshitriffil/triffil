package com.triffil2;

public class C {

	public static void main(String[] args) {
		
		String s1="th2is2";
		String s2="i2s";
		String s3="m2y";
		String s4="num3ber2";
		System.out.println(s1+" "+s2+" "+s3+" "+s4);
		
		
		char c[]=s1.toCharArray();
		char t=c[0];
		c[0]=c[s1.length()-1];
		c[s1.length()-1]=t;
		String s5=new String (c);
     System.out.println(s5);
     
		
	}

}
